{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/ai/(.*)$",
      "dest": "/app.js"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/app.js"
    },
    {
      "src": "^/(health|env-check|version|test)$",
      "dest": "/app.js"
    },
    {
      "src": "^/js/(.*)$",
      "dest": "/public/js/$1"
    },
    {
      "src": "^/css/(.*)$",
      "dest": "/css/$1"
    },
    {
      "src": "^/src/(.*)$",
      "dest": "/src/$1"
    },
    {
      "src": "^/uploads/(.*)$",
      "dest": "/app.js"
    },
    {
      "src": "^/projects/(.*)$",
      "dest": "/app.js"
    },
    {
      "src": "^/.*$",
      "dest": "/app.js"
    }
  ]
}
