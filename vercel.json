{
  "installCommand": "npm install --ignore-scripts",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist",
  "functions": {
    "api/[...all].js": {
      "includeFiles": "server/**"
    },
    "api/integrations/cw/health.js": {
      "includeFiles": "api/integrations/cw/**"
    },
    "api/integrations/cw/test-save/[formId].js": {
      "includeFiles": "api/integrations/cw/**"
    },
    "api/integrations/cw/measurements/[formId].js": {
      "includeFiles": "api/integrations/cw/**"
    },
    "api/integrations/cw/images/[formId].js": {
      "includeFiles": "api/integrations/cw/**"
    },
    "api/integrations/cw/images/[formId]/upload.js": {
      "includeFiles": "api/integrations/cw/**"
    },
    "api/integrations/cw/images/[formId]/delete.js": {
      "includeFiles": "api/integrations/cw/**"
    },
    "api/measurement-guides/svg.js": {
      "includeFiles": "server/**"
    },
    "api/storage/r2/health.js": {
      "includeFiles": "server/**"
    },
    "api/storage/r2/presign-upload.js": {
      "includeFiles": "server/**"
    },
    "api/storage/r2/signed-url.js": {
      "includeFiles": "server/**"
    },
    "api/storage/r2/delete.js": {
      "includeFiles": "server/**"
    },
    "api/storage/r2/copy.js": {
      "includeFiles": "server/**"
    },
    "api/storage/r2/object.js": {
      "includeFiles": "server/**"
    },
    "api/measurements/generate.js": {
      "includeFiles": "server/**",
      "maxDuration": 60
    }
  }
}
